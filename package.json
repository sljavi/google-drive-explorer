{
  "name": "google-drive-explorer",
  "version": "1.0.0",
  "description": "Just a google drive explorer example",
  "main": "index.js",
  "scripts": {
    "test": "mocha --recursive --compilers .:tests/compiler.js tests/app/*",
    "watch": "webpack-dev-server --colors --progress --content-base build/",
    "start": "node ./scripts/server.js",
    "plato": "node ./scripts/transform-jsx.js && npm run report",
    "report": "./node_modules/plato/bin/plato -r -d ./build/report -e .plato.eslintrc tmp/app/*",
    "build": "webpack --config ./webpack-prod.config.js --progress --colors",
    "postinstall": "npm run build && npm run coverage",
    "coverage": "mocha --recursive --compilers .:tests/compiler.js --require blanket --reporter html-cov tests/app/* > build/coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sljavi/google-drive-explorer.git"
  },
  "keywords": [
    "google",
    "drive",
    "example"
  ],
  "author": "Javier Perez",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sljavi/google-drive-explorer/issues"
  },
  "engines": {
    "node": "0.12.x"
  },
  "homepage": "https://github.com/sljavi/google-drive-explorer#readme",
  "devDependencies": {
    "blanket": "^1.1.7",
    "coveralls": "^2.11.4",
    "eslint": "1.6.0",
    "eslint-plugin-react": "3.5.1",
    "glob": "^5.0.15",
    "jsdom": "^3.1.1",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "mocha-lcov-reporter": "^1.0.0",
    "plato": "sljavi/plato"
  },
  "dependencies": {
    "async": "1.4.2",
    "bootstrap": "3.3.5",
    "classnames": "2.1.5",
    "css-loader": "0.19.0",
    "express": "4.13.3",
    "file-loader": "0.8.4",
    "google-client-api": "1.0.4",
    "jquery": "2.1.4",
    "jsx-loader": "0.13.2",
    "lodash": "3.10.1",
    "mkpath": "^0.1.0",
    "path": "0.12.7",
    "react": "0.13.3",
    "react-loader": "1.4.0",
    "react-tools": "^0.13.3",
    "style-loader": "0.12.4",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.0"
  },
  "config": {
    "blanket": {
      "pattern": "app",
      "data-cover-never": [
        "build",
        "node_modules",
        "tests"
      ],
      "data-cover-flags": {
        "debug": false
      },
      "data-cover-reporter-options": {
        "shortnames": true,
        "relativepath": true
      },
      "loader": "../../../tests/blanket-stub-jsx.js"
    }
  }
}
